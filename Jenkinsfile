pipeline {
	agent any 
	    stages{
	    	stage('1-make a left'){
	    		steps{
	    			echo "walk"	
	    		}
	    	}
	    	stage('2-make a right'){
	    		steps{
	    			echo "walk"
	    		}
	    	}
	    	stage('3-makeanother left'){
	    		steps{
	    			echo "walk"
	    		}
	    	}
	    	stage('4-cross'){
	    		steps{
	    			sh 'echo "walk"'
	    		}
	    	}
	    }
		
	
}
